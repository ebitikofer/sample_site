    <script language="javascript" type="text/javascript">
        function bookmark(url, title) {
            if ((navigator.appName == "Microsoft Internet Explorer") && (parseInt(navigator.appVersion) >= 4)) {
                window.external.AddFavorite(url, title);
            } else if (navigator.appName == "Netscape") {
                window.sidebar.addPanel(title, url, "");
            } else {
                // alert("Press CTRL-D (Netscape) or CTRL-T (Opera) to bookmark");
                var a = document.createElement("A");
                a.rel = "sidebar";
                a.target = "_search";
                a.title = title;
                a.href = url;
                a.click();
            }
        }
        function success(result) {
            setAnalytics(result);
            __doPostBack("ctl00$bodyContent$btnLogin", "");
        }

        function error(result) {
            setAnalytics(result);
            __doPostBack("ctl00$bodyContent$btnLogin", "");
        }

        function setAnalytics(result) {            
            document.getElementById("ctl00_bodyContent_txtAnalytics").value = JSON.stringify(result);;
        }

        function getSyncLock() {
            return window.synclock;
        }

        function performLogin() {
            $.ajaxSetup({ cache: false });
            var url = "https://api.onecallnow.com/MessengerAPI/1.0/token";
            var txtID = document.getElementById("ctl00_bodyContent_txtID");
            var txtPW = document.getElementById("ctl00_bodyContent_txtPassword");
            var data = "grant_type=password&client_id=2B8637A2-AB46-446C-9151-94552F2D9D9E&username=" + txtID.value + "&password=" + txtPW.value;
            $.ajax({
                type: "POST",
                url: url,
                data: data,
                success: success,
                error: error,
                xhrFields: {
                    withCredentials: true
                }
            });

            return false;
        }

    </script>
